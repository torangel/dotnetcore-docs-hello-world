@page
@model IndexModel
@{    
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Hello World from .NET 8</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <hr />
    <div>Host operating system: @Model.OSVersion</div>
    <div><b>Dmz:</b> @Model.Dmz </div>
    <div><b>DmzInternal:</b> @Model.DmzInternal </div>
    <div><b>Internal:</b> @Model.Internal </div>
    <div><b>My external IP</b>@Model.ExternalIp</div>
</div>
